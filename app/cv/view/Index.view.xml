<mvc:View
  height="100%"
  controllerName="com.tmhew.app.cv.controller.Index"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:m="sap.m"
  xmlns:uxap="sap.uxap">
  
  <m:Shell>
    <uxap:ObjectPageLayout
      showTitleInHeaderContent="false"
      showEditHeaderButton="false">
      <uxap:headerTitle>
        <uxap:ObjectPageDynamicHeaderTitle>
          <uxap:actions>
            <m:Button icon="sap-icon://source" tooltip="Source" press=".onSourceCodeButtonPressed"/>
            <m:Button icon="sap-icon://dark-mode" tooltip="Theme" press=".onThemeButtonPressed"/>
          </uxap:actions>
        </uxap:ObjectPageDynamicHeaderTitle>
      </uxap:headerTitle>
    
    </uxap:ObjectPageLayout>
  </m:Shell>
  
</mvc:View>
