{
    "sap.app": {
        "id": "com.md.Component",
        "type": "application",
        "i18n": "i18n/Component.properties",
        "title": "{{TITLE}}",
        "description": "{{DESCRIPTION}}"
    },

    "sap.ui": {
        "technology": "UI5"
    },

    "sap.ui5": {
        "dependencies": {
            "libs": {
                "com.md.library.paper": {}
            }
        },

        "resources": {
            "css": [
                { "uri": "../../bower_components/normalize.css/normalize.css" }
            ]
        },

        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
    			"settings": {
    				"bundleName": "com.md.i18n.Component"
    			}
            }
        },

        "rootView": {
            "viewName": "com.md.view.App",
            "type": "XML"
        },

        "routing": {
            "config": {
                "viewType": "XML",
                "controlAggregation": "content",
                "clearControlAggregation": true
            },

            "routes": {
                "notFound": {
                    "pattern": "{all*}",
                    "target": [ "" ]
                }
            },

            "targets": {

            }
        }

    }
}
