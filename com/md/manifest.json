{
    "sap.app": {
        "id": "com.md.Component",
        "type": "application"
    },

    "sap.ui": {
        "technology": "UI5"
    },

    "sap.ui5": {
        "rootView": {
            "viewName": "com.md.view.App",
            "type": "XML"
        },

        "routing": {
            "config": {
                "viewType": "XML",
                "controlAggregation": "content",
                "clearControlAggregation": true
            },

            "routes": {
                "notFound": {
                    "pattern": "{all*}",
                    "target": [ "" ]
                }
            },

            "targets": {

            }
        }
    }
}
