{
    "sap.app": {
        "id": "com.ui5.md.Component",
        "type": "application",
        "i18n": "i18n/Component.properties",
        "title": "{{TITLE}}",
        "description": "{{DESCRIPTION}}",
        "dataSources": {
            "northwind": {
                "uri": "http://services.odata.org/V2/Northwind/Northwind.svc/",
    			"type": "OData",
    			"settings": {
    				"odataVersion": "2.0"
    			}
            }
        }
    },

    "sap.ui": {
        "technology": "UI5"
    },

    "sap.ui5": {
        "dependencies": {
            "libs": {
                "com.ui5.md.library.paper": {}
            }
        },

        "resources": {
            "css": [
                { "uri": "../../../bower_components/normalize.css/normalize.css" }
            ]
        },

        "models": {
            "i18n.App": {
                "type": "sap.ui.model.resource.ResourceModel",
    			"settings": {
    				"bundleName": "com.ui5.md.i18n.App"
    			}
            },
            "comp.northwind": {
                "dataSource": "northwind"
            }
        },

        "rootView": {
            "viewName": "com.ui5.md.view.App",
            "type": "XML"
        },

        "routing": {
            "config": {
                "viewType": "XML",
                "controlAggregation": "content",
                "clearControlAggregation": true
            },

            "routes": {
                "notFound": {
                    "pattern": "{all*}",
                    "target": [ "" ]
                }
            },

            "targets": {

            }
        }

    }
}
