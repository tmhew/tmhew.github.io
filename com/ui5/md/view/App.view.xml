<mvc:View
    controllerName="com.ui5.md.controller.App"
    xmlns="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    height="100%"
    width="100%"
    displayBlock="true">

    <m:Table items="{comp.northwind>/Employees}">
        <m:items>
            <m:ColumnListItem>
                <m:cells>
                    <m:Text text="{comp.northwind>LastName}"/>
                </m:cells>
            </m:ColumnListItem>
        </m:items>
    </m:Table>

</mvc:View>
