{
    "_version": "1.1.0",

    "sap.app": {
	    "_version": "1.1.0",
	    "id": "nest.comp.child",
        "type": "component",
        "title": "child",
        "description": "child component",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },

    "sap.ui": {
	    "_version": "1.1.0",
	    "technology": "UI5",
	    "deviceTypes": {
            "desktop": true,
	        "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_belize"
        ]
    },

    "sap.ui5": {
        "_version": "1.1.0",
        "componentName": "nest.comp.child",
        "rootView": "nest.comp.child.view.Index",
        "dependencies": {
            "minUI5Version": "1.40",
            "libs": {
                "sap.m": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "async": true,
                "viewType": "XML",
                "viewPath": "nest.comp.child.view",
                "controlId": "indexApp",
                "controlAggregation": "pages"
            },
            "routes": [
                {
                    "name": "main",
                    "pattern": "main",
                    "target": "main",
                    "parent": "nest:child"
                }
            ],
            "targets": {
                "main": {
                    "viewName": "Child"
                }
            }
        }
    }
}
